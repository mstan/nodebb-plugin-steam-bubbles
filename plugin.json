{
	"id": "nodebb-plugin-steam-bubbles",
	"name": "Steam Bubbles",
	"description": "Add Steam Bubbles to user posts",
	"url": "https://github.com/mstan",
	"library": "./library.js",
	"hooks": [
		{
			"hook": "static:app.load", "method": "init"
		},
		{
			"hook": "filter:posts.custom_profile_info", "method": "addProfileInfo"
		}
	],
	"staticDirs": {
		"static": "./static"
	}
}